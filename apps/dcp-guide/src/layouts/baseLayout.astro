---
import DefaultGuideLayout from './defaultGuideLayout.astro';
import HeroImage from '../components/HeroImage.astro';
import HeroHome from '../components/HeroHome.astro';
export interface Props {
  title: string;
}
const { title } = Astro.props;
---

<DefaultGuideLayout title="Welcome to DCP Guide.">
  {title !== 'Home' && <HeroImage title={title} />}

  {title == 'Home' && <HeroHome />}
  <slot />
</DefaultGuideLayout>
